<question type="multichoice">
  <name><text>Cheapest Product Algorithm</text></name>
  <questiontext format="html">
    <text><![CDATA[What is the correct initial assumption when finding the cheapest product in an ArrayList?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>Assume the first product is the cheapest</text>
    <feedback><text><![CDATA[Correct. This provides a valid starting comparison.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>Assume the last product is the cheapest</text>
    <feedback><text><![CDATA[Incorrect. There is no guarantee.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>Assume all products cost the same</text>
    <feedback><text><![CDATA[Incorrect. Costs must be compared.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>Sort the ArrayList first</text>
    <feedback><text><![CDATA[Incorrect. Sorting is unnecessary.]]></text></feedback>
  </answer>
</question>

<!-- Q11 -->
<question type="multichoice">
  <name><text>isEmpty vs size</text></name>
  <questiontext format="html">
    <text><![CDATA[Which condition correctly checks if an ArrayList is empty?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>products.isEmpty()</text>
    <feedback><text><![CDATA[Correct. isEmpty() directly checks if the list has no elements.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>products.size() &lt; 0</text>
    <feedback><text><![CDATA[Incorrect. size() is never negative.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>products == null</text>
    <feedback><text><![CDATA[Incorrect. This checks the reference, not contents.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>products.length == 0</text>
    <feedback><text><![CDATA[Incorrect. length is for arrays, not ArrayLists.]]></text></feedback>
  </answer>
</question>

