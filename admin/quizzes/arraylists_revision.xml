<?xml version="1.0" encoding="UTF-8"?>
<quiz>

<!-- Q1 -->
<question type="multichoice">
  <name><text>ArrayList Purpose</text></name>
  <questiontext format="html">
    <text><![CDATA[Why are ArrayLists preferred over arrays when grouping objects?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>They can grow and shrink dynamically</text>
    <feedback><text><![CDATA[Correct. ArrayLists automatically manage their size.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>They store elements in fixed memory locations</text>
    <feedback><text><![CDATA[Incorrect. Fixed memory locations apply to arrays.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>They store primitive types directly</text>
    <feedback><text><![CDATA[Incorrect. ArrayLists store objects only.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>They are faster than arrays in all cases</text>
    <feedback><text><![CDATA[Incorrect. Performance depends on usage.]]></text></feedback>
  </answer>
</question>

<!-- Q2 -->
<question type="multichoice">
  <name><text>Generic Type Parameter</text></name>
  <questiontext format="html">
    <text><![CDATA[What does &lt;String&gt; mean in ArrayList&lt;String&gt;?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>It specifies the type of objects stored in the ArrayList</text>
    <feedback><text><![CDATA[Correct. This makes the ArrayList type-safe.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>It creates a new String object</text>
    <feedback><text><![CDATA[Incorrect. No object is created here.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>It limits the size of the ArrayList</text>
    <feedback><text><![CDATA[Incorrect. Size is managed automatically.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>It converts primitives into objects</text>
    <feedback><text><![CDATA[Incorrect. This is unrelated to generics.]]></text></feedback>
  </answer>
</question>

<!-- Q3 -->
<question type="multichoice">
  <name><text>Importing ArrayList</text></name>
  <questiontext format="html">
    <text><![CDATA[Which import statement is required to use ArrayList?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>import java.util.ArrayList;</text>
    <feedback><text><![CDATA[Correct. ArrayList belongs to the java.util package.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>import java.lang.ArrayList;</text>
    <feedback><text><![CDATA[Incorrect. java.lang does not contain ArrayList.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>import java.io.ArrayList;</text>
    <feedback><text><![CDATA[Incorrect. java.io is for I/O classes.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>No import is required</text>
    <feedback><text><![CDATA[Incorrect. ArrayList must be imported.]]></text></feedback>
  </answer>
</question>

<!-- Q4 -->
<question type="multichoice">
  <name><text>Adding Elements</text></name>
  <questiontext format="html">
    <text><![CDATA[Which method is used to add an element to an ArrayList?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>add()</text>
    <feedback><text><![CDATA[Correct. add() inserts an element into the list.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>insert()</text>
    <feedback><text><![CDATA[Incorrect. insert() is not an ArrayList method.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>append()</text>
    <feedback><text><![CDATA[Incorrect. append() does not exist for ArrayList.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>put()</text>
    <feedback><text><![CDATA[Incorrect. put() is used with maps.]]></text></feedback>
  </answer>
</question>

<!-- Q5 -->
<question type="multichoice">
  <name><text>size Method</text></name>
  <questiontext format="html">
    <text><![CDATA[What does the size() method of an ArrayList return?]]></text>
  </questiontext>
  <answer fraction="100">
    <text>The number of elements currently stored</text>
    <feedback><text><![CDATA[Correct. size() tracks how many items are stored.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>The maximum capacity</text>
    <feedback><text><![CDATA[Incorrect. Capacity is hidden.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>The last valid index</text>
    <feedback><text><![CDATA[Incorrect. Indices start at 0.]]></text></feedback>
  </answer>
  <answer fraction="0">
    <text>The memory size</text>
    <feedback><text><![CDATA[Incorrect. Memory size is not exposed.]]></text></feedback>
  </answer>
</question>

<!-- Q6 -->
<question type="multichoice">
  <name><text>get Method</text></name>
  <questiontext format="html">
    <text><![CDATA[What does notes.get(2) do?]]></text>
  </questiontext>
  <answer fraction="100">
